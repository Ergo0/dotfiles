{
   "background": {
      "scripts": [ "lib/app.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "data/off-16.png",
         "32": "data/off-32.png",
         "64": "data/off-32.png"
      },
      "default_title": "Media Hint: OFF"
   },
   "description": "Use the Media Hint service to access the content you need on the internet.",
   "externally_connectable": {
      "matches": [ "https://mediahint.com/*" ]
   },
   "icons": {
      "128": "data/icon.png",
      "48": "data/icon-48.png",
      "96": "data/icon-96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Media Hint",
   "permissions": [ "proxy", "https://mediahint.com/*", "webRequest", "webRequestBlocking", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1.1"
}
